<h1>Editing Equipment</h1>

<% form_for(@equipment) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :equipment_name %><br />
    <%= f.text_field :name %>
  </p>
  
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @equipment %> |
<%= link_to 'Back', equipments_path %>
